{
    "expo": {
        "name": "mobile",
        "slug": "mobile",
        "version": "1.0.3",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "dev.hcdaly.lilypad"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
